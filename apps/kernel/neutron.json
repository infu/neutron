{
  "id": "kernel",
  "name": "Neutron",
  "version": 1,
  "src": "main.mo",
  "init_arg": [
    "memory_kernel"
  ],
  "func": {
    "kernel_authorized_add": {
      "type": "update",
      "async": false
    },
    "kernel_authorized_rem": {
      "type": "update",
      "async": false
    },
    "is_authorized": {
      "type": "internal",
      "async": false
    },
    "kernel_static": {
      "type": "update",
      "async": false
    },
    "kernel_static_query": {
      "type": "query",
      "async": false
    },
    "http_request": {
      "type": "query",
      "async": false,
      "arg": [
        "caller",
        "this"
      ],
      "allow": "unauthorized"
    },
    "http_request_streaming_callback": {
      "type": "query",
      "async": false,
      "allow": "unauthorized"
    },
    "kernel_install_code": {
      "type": "update",
      "async": true,
      "arg": [
        "this"
      ]
    }
  },
  "memory": {
    "kernel": {
      "version": 1
    }
  }
}